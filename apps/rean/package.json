{
  "name": "rean",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --port 8082",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "ios:debugdev": "npx expo run:ios --scheme rean_dev --configuration DebugDev",
    "ios:debuguat": "npx expo run:ios --scheme rean_uat --configuration DebugUAT",
    "ios:release": "npx expo run:ios --scheme rean --configuration Debug",
    "web": "expo start --web",
    "test:watch": "jest --watchAll",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(?:.pnpm/)?((jest-)?react-native|@react-native(-community)?|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@sentry/react-native|native-base|react-native-svg))"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/bottom-tabs": "^7.3.12",
    "@react-navigation/native": "^7.1.8",
    "@tanstack/react-query": "^5.76.1",
    "axios": "^1.8.4",
    "expo": "~53.0.7",
    "expo-blur": "~14.1.4",
    "expo-camera": "^16.1.6",
    "expo-constants": "~17.1.5",
    "expo-document-picker": "~13.1.5",
    "expo-file-system": "~18.1.10",
    "expo-haptics": "~14.1.4",
    "expo-linear-gradient": "~14.1.4",
    "expo-linking": "~7.1.4",
    "expo-localization": "~16.1.5",
    "expo-media-library": "~17.1.6",
    "expo-router": "~5.0.5",
    "expo-secure-store": "~14.2.3",
    "expo-sharing": "~13.1.5",
    "expo-splash-screen": "~0.30.8",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.4",
    "expo-system-ui": "~5.0.7",
    "expo-web-browser": "~14.1.6",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.2",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-otp-entry": "^1.8.4",
    "react-native-reanimated": "~3.17.5",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.10.0",
    "react-native-svg": "^15.11.2",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@react-native-community/cli": "latest",
    "@repo/colors": "workspace:*",
    "@repo/eslint-config": "workspace:*",
    "@repo/ums-agent": "workspace:*",
    "@testing-library/react-native": "^13.2.0",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.0.14",
    "@types/react-test-renderer": "^18.3.1",
    "jest": "^29.7.0",
    "jest-expo": "~53.0.4",
    "react-test-renderer": "18.3.1",
    "typescript": "^5.8.3"
  },
  "private": true
}
