{
  "expo": {
    "name": "rean",
    "slug": "rean",
    "orientation": "portrait",
    "icon": "./assets/images/logo/rean_light.png",
    "resizeMode": "contain",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "kh.gov.rean"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo/rean_light.png",
        "backgroundColor": "#ffffff"
      },
      "package": "kh.gov.rean"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo/rean_light.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo/rean_light.png",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-localization",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera"
        }
      ],
      "expo-font",
      "expo-web-browser",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
